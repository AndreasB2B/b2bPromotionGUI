@using KN.B2B.Model
@model IList<Customer>


<table class="table">
    <thead>
        <tr>
            <th width="15%">
                @Html.DisplayNameFor(model => model[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].B2BResponsible)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Att)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Telephone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].CVR)
            </th>
            <th width="13%"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var lead in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => lead.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => lead.Id)
                </td>
                <td>
                    @if (lead.B2BResponsible != null)
                    {
                        @Html.DisplayFor(modelItem => lead.B2BResponsible.Name)
                    }
                    else
                    {
                        @Html.DisplayFor(modelItem => lead.B2BResponsible)
                    }
                </td>
                <td>
                    @Html.DisplayFor(modelItem => lead.Att)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => lead.Telephone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => lead.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => lead.CVR)
                </td>
                <td style="text-align:right" width="13%">
                    <a class="btn btn-lg" asp-page="./Edit" asp-route-id="@lead.Id">
                        <i class="fas fa-info-circle"></i>
                    </a>
                    <a class="btn btn-lg" asp-page="./Delete" asp-route-id="@lead.Id">
                        <i class="fas fa-trash"></i>
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
