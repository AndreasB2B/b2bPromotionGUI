@using KN.B2B.Model.SystemTables
@model B2BCategory

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input disabled asp-for="Id" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Category" class="control-label"></label>
                <input disabled asp-for="Category" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CategoryDK" class="control-label"></label>
                <input disabled asp-for="CategoryDK" class="form-control" />
            </div>
            <label asp-for="CategoryGroup" class="control-label"></label>
            <select disabled class="form-control" asp-for="CategoryGroup">
                @if (Model.CategoryGroup != null)
                {
                    <option selected>@Model.CategoryGroup.CategoryGroup</option>
                }
            </select>
        </form>
    </div>
</div>